summarise(viv_n_ambientes_mean = mean(viv_n_ambientes),
hog_n_miembros_hogar_mean = mean(hog_n_miembros_hogar))
hacinamiento_agregado <- hacinamiento_agregado_intermedio |>
summarise("Hacinamiento agregado" = round(mean(hog_n_miembros_hogar_mean / viv_n_ambientes_mean), 1)) |>
tbl_custom_summary(
include = "Hacinamiento agregado",
stat_fns = ~ continuous_summary("Hacinamiento agregado"),
statistic = ~"{mean}")
