df_encuesta <- df_encuesta |>
mutate(unaj_comision = str_c(unaj_ano_lectivo,
unaj_periodo_lectivo,
cuanti_docente_siglas,
sep = "-"),
unaj_comision = as_factor(unaj_comision)) |>
relocate(unaj_comision, .after = unaj_periodo_lectivo)
